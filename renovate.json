{
  "extends": [
    "config:base"
  ],
  "vulnerabilityAlerts": {
    "enabled": true
  },
  "automerge": true,
  "major": {
    "automerge": false
  },
  "labels": [
    "type: dependencies",
    "renovate"
  ],
  "masterIssue": true,
  "prConcurrentLimit": 8,
  "prHourlyLimit": 4,
  "timezone": "America/New_York",
  "schedule": [
    "every weekend"
  ],
  "github-actions": {
    "enabled": true,
    "labels": [
      "no release",
      "renovate-deps"
    ],
    "automerge": true
  }
}
